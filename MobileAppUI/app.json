{
  "expo": {
    "name": "MobileAppUI",
    "slug": "MobileAppUI",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "backgroundColor": "#BCD042",
    "icon": "./assets/images/splash-icon.png",
    "scheme": "mobileappui",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amitavpanda.MobileAppUI"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#BCD042"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.amitavpanda.MobileAppUI"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#BCD042"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ceac7e11-b4e3-444d-82c4-745b1788c257"
      }
    }
  }
}
