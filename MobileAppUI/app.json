{
  "expo": {
    "name": "MobileAppUI",
    "slug": "MobileAppUI",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "backgroundColor": "#BCD042",
    "icon": "./assets/images/splash-icon.png",
    "scheme": "mobileappui",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "projectId": "34cbcac0-891d-4910-b667-1d0762dfa52b",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amitavpanda.MobileAppUI",
      "config": {
        "googleMapsApiKey": "process.env.GOOGLE_MAPS_API_KEY_IOS"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#BCD042"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.amitavpanda.MobileAppUI",
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "process.env.EXPO_GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#BCD042"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "34cbcac0-891d-4910-b667-1d0762dfa52b"
      },
      "GOOGLE_MAPS_API_KEY_ANDROID": "AIzaSyCLugNuaKypDiHsK8_aWubkwFUO893_SQc",
      "GOOGLE_MAPS_API_KEY_IOS": "AIzaSyCWsxdSVg2E95OJOJNELLhrs9pyGFWwZ2o",
      "GOOGLE_MAPS_API_KEY": "AIzaSyCWsxdSVg2E95OJOJNELLhrs9pyGFWwZ2o",
      "EXPO_PUBLIC_API_URL": "http://10.107.56.114:3002"
    }
  }
}
